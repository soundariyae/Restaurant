<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mvc="http://www.springframework.org/schema/mvc"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">



	<bean class="com.acc.controller.LoginController" scope="session">
		<property name="loginDelegate" ref="loginDelegate"></property>
		
	</bean>
	<bean id="loginDelegate" class="com.acc.delegate.impl.LoginDelegateImpl"></bean>

	<bean class="com.acc.controller.AdminViewController" scope="session">
		
		<property name="adminViewDelegate" ref="adminViewDelegate"></property>
	</bean>
	<bean id="adminViewDelegate" class="com.acc.delegate.impl.AdminViewDelegateImpl"></bean>
</beans>